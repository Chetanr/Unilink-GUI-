<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="279.0" prefWidth="521.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Login">
   <children>
      <Label layoutX="108.0" layoutY="140.0" text="Username" />
      <TextField fx:id="userName" layoutX="184.0" layoutY="135.0" promptText="Enter your username" />
      <Button fx:id="login" layoutX="199.0" layoutY="190.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <Label fx:id="validate" layoutX="353.0" layoutY="142.0" text="Label" textFill="#e80909">
         <font>
            <Font size="11.0" />
         </font>
      </Label>
      <Label layoutX="146.0" layoutY="62.0" text="Welcome! Please login">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
