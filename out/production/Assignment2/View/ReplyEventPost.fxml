<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ReplyEventPost">
   <children>
      <Label layoutX="268.0" layoutY="90.0" text="Title :">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <Label layoutX="239.0" layoutY="141.0" text="Description :">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Label layoutX="238.0" layoutY="222.0" prefHeight="17.0" prefWidth="221.0" text="Do you want to attend the event?">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button fx:id="yes" layoutX="256.0" layoutY="275.0" mnemonicParsing="false" onAction="#confirmAttendence" text="Yes" />
      <Button fx:id="No" layoutX="380.0" layoutY="275.0" mnemonicParsing="false" onAction="#notAttending" text="No" />
      <Label fx:id="title" layoutX="326.0" layoutY="91.0" />
      <Label fx:id="description" layoutX="333.0" layoutY="142.0" />
      <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="200.0" layoutX="14.0" layoutY="60.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
