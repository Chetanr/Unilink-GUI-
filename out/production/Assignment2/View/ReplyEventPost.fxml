<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ReplyEventPost">
   <children>
      <Label layoutX="253.0" layoutY="276.0" prefHeight="17.0" prefWidth="221.0" text="Do you want to attend the event?">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button fx:id="yes" layoutX="271.0" layoutY="329.0" mnemonicParsing="false" onAction="#confirmAttendence" text="Yes" />
      <Button fx:id="no" layoutX="395.0" layoutY="329.0" mnemonicParsing="false" onAction="#notAttending" text="No" />
      <Label fx:id="title" layoutX="319.0" layoutY="93.0" text="title">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Label>
      <Label fx:id="description" layoutX="316.0" layoutY="135.0" text="description">
         <font>
            <Font size="18.0" />
         </font></Label>
      <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="200.0" layoutX="14.0" layoutY="60.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="venue" layoutX="311.0" layoutY="171.0" text="venue">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="date" layoutX="316.0" layoutY="201.0" text="date">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
